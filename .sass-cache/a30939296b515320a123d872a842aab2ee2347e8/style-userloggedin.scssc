:@template"G4$blue-top : #2e81cc;
$blue-bottom : #1f70bb;
$blue-dark : #01488a;
$blue-link : #2071bc;

$green-bottom : #4e983d;
$green-top : #58ab3c;
$green-dark : #367b27;

$red-bottom : #cb4101;
$red-top  : #da5517;
$red-dark : #a73601;

$highlight : #FEFEE4;

body, html{
	background:#f8f8f8
}

.topbar{
	text-align:right;
	border-top:solid 3px #757575;			
}
.topbar .topcontent{
	width:800px;
	margin:auto;
	text-align:center;
}


.topcontent a{
	font-size:9px;
	font-weight:bold;
	color: #fff;
	display:inline-block;
	margin:0px 2px;
	padding:0px 7px 4px 7px!important;
	vertical-align: top;
	line-height: 1.0em !important;
	-webkit-border-bottom-right-radius: 3px;
	-webkit-border-bottom-left-radius: 3px;
	-moz-border-radius-bottomright: 3px;
	-moz-border-radius-bottomleft: 3px;
	border-bottom-right-radius: 3px;
	border-bottom-left-radius: 3px;
}

.topcontent a.logout{
	background:$red-top;
	float:right;
	text-shadow: 1px 1px 1px $red-dark;
}
.content{
	width:800px;
	margin:auto;
	text-align:left;
	position:relative;
}

.header{
	text-align:left;
	border-bottom:dashed 1px #ddd;
	width:800px;
	position:relative;
	margin:0px auto 5px auto;
}

.welcome{
	float:left;
	font-size:20px;
	font-weight:bold;
	color:#333;
	display:inline-block;	
}

.header nav{
	float:right;
	text-align:right;
	display:inline-block;
}

.header .usernav a{
	color:#555;
	font-size:11px;
	font-weight:bold;
	margin:0px 0px 0px 3px;
	display:inline-block;
	background:#f8f8f8;
	border:dashed 1px #ddd;
	padding:5px;	
}

.header nav a.active{
	color:#fff;
	text-shadow:0px 1px 1px $blue-dark;
	background:$blue-top;
	border:solid 1px $blue-bottom;
}


a.logo{
	text-indent:-10000px;
	display:inline-block;
	float:left;
	margin:auto;
	background: url('../images/sprite.png') no-repeat;
}

a.logo.micro{
	height:27px;
	width:90px;
	border:solid 0px $red-top;
	background-position: 0px -88px;
}

.content.welcome{
	width:700px;
}

.franklin{
	float:right;
	height:260px;
	width:210px;
	border:solid 0px #ddd;
	margin-top:0px;
	background: url('../images/sprite.png') -205px 0px no-repeat;			
}

h1{
	font-size:28px;
	font-weight:normal;
	margin:20px auto 0px auto;
	text-align:left;
}

p.large{
	font-size:17px;
	line-height:1.5em;
	margin:10px auto 30px auto;
}
p.large em{
	font-size:17px
}


p.large.welcome{
	margin:20px 230px 10px 0px !important;
}



.icon{
	float:right;
	height:215px;
	width:205px;
	margin-left:30px;
	background: url('../images/sprite.png') 0px -120px no-repeat;
}

.notes{
	padding:15px;
	margin-bottom:20px;
	background:#f4f4f4;
	border:solid 1px #efefef;
}
.notes li{
	font-size:14px;
	margin:0px 0px 10px 20px;
}



.welcomelinks{
	text-align:center;
}




.virtues{
	width:42%;
	margin:0px 10px
}

.virtues.right{
	float:right;
}
.virtues.left{
	float:left;
}

.virtue{
	border:solid 1px #f8f8f8;
	margin:0px 0px 10px 0px
}

.virtue:hover{
	cursor:hand;
	cursor:pointer;
	border:solid 1px #ecebeb;
	background:#f4f4f4;
}

.virtue em{
	background:$highlight;
}

.virtue span{
	font-size:10px;
	display:block;
	color:#777;
	margin-left:25px;
}

.logentry h1{
	font-size:18px;
	margin-bottom:10px;
}

			
.entryHeader{
	float:left;
}

.entryRecap{
	text-align:center;
	width:auto;
}
.entryRecap .notes{
	padding:10px;
}
.entryRecap h2{
	font-size:11px;
}
.entryRecap h2 span{
	font-size:22px;
	font-weight:bold;
	line-height:1.0;				
}

.entryRecap{
	float:right;
}


.logentry h1 em{
	font-size:18px;
	font-weight:bold;
}

.slider{
	padding-left:20px;
}

.slider span.label{
	font-size:15px;
	font-weight:bold;
	float:left;
	display:block;
	width:60%;
}

.slider span.label em.description{
	font-size:10px; 
	color:#777;
	display:block;
	font-weight:normal;
}

.slider span.label em.description em{
	background:$highlight;
}

.slider input[type="text"]{
	color:333;
	width:25px;
	height:20px;
	padding:5px;
	font-size:17px;
	font-weight:bold;
	
	float:left;
	margin-left:10px;
	text-align:center;
	
  	border-radius: 3px;
  	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	
	box-shadow: inset 1px 1px #ddd;
  	-moz-box-shadow: inset 1px 1px #ddd;
  	-webkit-box-shadow: inset 1px 1px #ddd;

	background: #edf5f5;
	background: -moz-linear-gradient(bottom, #edf5f5 0%, #fefefe 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #fefefe), color-stop(100%, #edf5f5));
	background: -webkit-linear-gradient(top, #fefefe 0%, #edf5f5 100%);
	background: -o-linear-gradient(top, #fefefe 0%, #edf5f5 100%);
	background: -ms-linear-gradient(top, #fefefe 0%, #edf5f5 100%);
	background: linear-gradient(top, #fefefe 0%, #edf5f5 100%);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#fefefe', endColorstr='#edf5f5',GradientType=0 );	
}

.happinessScaleSlider{
	display:inline-block !important;
}

.slider .sliderValue{
	color:#777;
	font-size:10px;
	display:inline-block !important;
	border:solid 0px #ddd;
	vertical-align:middle;
	margin:0px 10px
}




.notesHeader{
	text-align:center;
	margin-top:10px;
}

.notesHeader span{
	font-weight:bold;
	display:inline-block;
	vertical-align:top;
	margin:0px 5px 5px 5px;
}

.logentry .notes{
	text-align:center;
}
.logentry .notes textarea{
	color:777;
	width:90%;
	height:100px;
	padding:5px;
	font-size:14px;
	font-family: Arial, Gill Sans, Arial;
	letter-spacing:0.03em;
	
	
	margin:auto;
	
	border-radius: 3px;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	
	box-shadow: inset 1px 1px #ddd;
	-moz-box-shadow: inset 1px 1px #ddd;
	-webkit-box-shadow: inset 1px 1px #ddd;
	border:solid 1px #ddd;
	
	background: #f8f8f8;
	/**
	background: -moz-linear-gradient(bottom, #edf5f5 0%, #fefefe 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #fefefe), color-stop(100%, #edf5f5));
	background: -webkit-linear-gradient(top, #fefefe 0%, #edf5f5 100%);
	background: -o-linear-gradient(top, #fefefe 0%, #edf5f5 100%);
	background: -ms-linear-gradient(top, #fefefe 0%, #edf5f5 100%);
	background: linear-gradient(top, #fefefe 0%, #edf5f5 100%);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#fefefe', endColorstr='#edf5f5',GradientType=0 );
	**/
}








/** dashboard styles **/

.dashboard h1{
	float:left;
	margin:10px 0px 0px 0px;
	vertical-align:middle;
	border:dotted 0px #ddd;
}

.dashboard h1 span{
	color:#777;
	font-style:italic;
	font-family:Gill Sans;
}

.userstats{
	float:right;
	padding:0px;
	margin:10px 0px 0px 0px;
	border:dotted 0px #ddd;
}

.userstats span{
	font-size:11px;
	color:#555;
	display:inline-block;
	margin:0px 0px 0px 30px;
	vertical-align:bottom;
}

.userstats em{
	background:$highlight;
	font-weight:bold;
	font-size:20px;
	color:#333;
	vertical-align:baseline;
}

.userstats em.scoreWrapper,
.userstats em.rankWrapper,
.userstats em.entriesWrapper{
	text-align:center;
	display:inline-block;
}
.userstats em.scoreWrapper{
	width:100px;
	text-align:right
}
.userstats em.entriesWrapper{
	width:40px
}

.userstats em.rankWrapper{
	width:40px;
}

.userstats em em{
	margin:auto
}




.yAxis.y1Axis .tickLabel{
	border:solid 0px $red-top;
	margin-right:5px !important;
}
.tickLabel{
	font-size:8px;
	color:#777;
}

.legendLabel{
	font-size:9px;
	color:#777;
}

.legendColorBox div div{
	width:3px !important;
	height: 0 !important;
	border-width:2px !important;
}


.fc-day-number{
	font-size:9px !important;
	color:#777 !important;
}

.fc-widget-header{
	font-size:9px !important;
	font-weight:bold !important;
	color:#555 !important;
}


#dashleft{
	width:510px; 
	width:780px;
	height:280px; 
	float:left; 
	padding:10px; 
	background:#f4f4f4; 
	border:solid 1px #ebeaea;
	margin:10px 0px 0px 0px;
}


#progress{
	width:500px; 
	width: 770px !important;
	height:250px; 
	margin:auto; 
	position:relative;
}

#calendarWrapper{
	float:right; 
	padding:10px;
	margin:10px 0px 0px 0px; 
	background:#f4f4f4; 
	border:solid 1px #ebeaea;
}

#calendar{
	width:200px; 
	height:240px; 
	background:#f8f8f8; 
	padding:10px;
	border:solid 0px #fff
}

.statsWrapper{ 
	margin:25px auto;
	padding:10px; 
	background:#f4f4f4; 
	border:solid 1px #ebeaea
}

.stats{
	width:350px;
	background:#f8f8f8; 
	padding:15px;
	border:solid 0px #fff
}

.stats.best{
	float:left;
}

.stats.worst{
	float:right;
}

.stats h2{
	font-size:22px;
	margin:5px 0px;
	padding:0px;
	line-height:1.0em;
}
.stats.best h2{
	color:$blue-dark;
}
.stats.worst h2{
	color:$red-dark;
}

.stats p{
	font-size:10px;
	color:#777;
	margin:0px;
}

.stats table{
	width:100%;
	margin:5px;
}

.stats table tbody tr{
	border-bottom:solid 1px #f1f1f1;
	border-top:solid 1px #f1f1f1;
}

.stats table th{
	font-size:8px;
	color:#777;
	font-weight:bold;
	letter-spacing:0.03em;
	text-align:center;
}

.stats table td.virtue{
	font-size:15px;
	padding:4px;
	cursor:default;
	border-bottom:solid 1px #f1f1f1;
	border-top:solid 1px #f1f1f1;
}

.stats table td.virtue:hover{
	background:inherit;
	border:inherit;
}

.stats table td.percent{
	font-size:17px;
	color:#333;
	font-weight:bold;
	text-align:right;
}

.stats table td.percent em{
	font-size:10px;
	display:inline-block;
	vertical-align:top;
	color:#777;
	margin:2px 0px 0px 5px;
}



.fc-button-content{
	font-size:8px !important;
	text-transform:uppercase;
	letter-spacing:0.03m;
}

.fc-state-down .fc-button-content{
	color:#fff !important;
}

.fc-event-inner{
	text-align:center;
}

.fc-event-inner span{
	font-size : 10px;
	font-weight:bold;
	text-shadow: 0px 1px 1px $blue-dark;
	letter-spacing:0.04em;
	color:#fff !important;
	text-align:center !important;
	display:inline-block;
}

.ui-tooltip{
	line-height:1.5em !important;
}



.qtiptitle{
	font-weight:bold;
	color:#ddd;
	text-shadow:0px 1px 1px #111;
	display:block;
}

.highlight{
	background:$highlight;
}

.qtipstat{
	color:#b5b5b5;
	font-size:11px;
	display:inline-block;
	margin:2px 4px;
	text-shadow:0px 1px 1px #111;
}
.qtipstat em{
	font-size:12px;
	font-weight:bold;
	color:$highlight ;
	color:#f8f8f8 !important;
}




h3{
	margin:0px;
	color:#333;
	font-size:15px;
	font-weight:bold;
	display:inline-block;
}

h3 span{
	font-size:9px;
	color:#777;
	margin-left:10px;
	font-weight:normal;
	display:inline-block !important;
}











/** history **/
.history h1{
	margin:10px 0px 0px 0px;
	vertical-align:middle;
	border:dotted 0px #ddd;
}

.history h1 span{
	color:#777;
	font-style:italic;
	font-family:Gill Sans;
}


#entriesListWrapper{
	width:510px; 
	float:left; 
	padding:10px; 
	height:auto;
	background:#f4f4f4; 
	border:solid 1px #ebeaea;
	margin:10px 0px 0px 0px;
}

table.entries{
	width:100%;
}

table.entries th{
	padding:3px;
	text-align:center;
	font-size:9px;
	color:#777;
	font-weight:bold;
	background:#efefef;
}

table.entries tr{
	background:#f4f4f4;
	width:100%;
	margin:0px;
	padding:0px;
}
table.entries tr.even{
	background:#f8f8f8;
}

table.entries td{
	text-align:center;
	font-size:11px;
	vertical-align:top;
	padding:8px 0px;
}

table.entries td em{
	font-size:17px;
	font-weight:bold;
}

table.entries td a{
	font-size:9px;
	letter-spacing:0.04em;
}


table.entries td.entryNotes{
	width:200px;
	text-align:left;
}

.history #calendar{
	/**width:300px !important; **/
	height: auto !important; 
}










/** account page **/
.accountContainer{
	width:800px
}

.accountContainer .franklin{
	margin-top:20px !important;
}
.accountContainer h1{
 	margin-bottom:0px;
	padding:0px;
}

.accountContainer p.large{
	margin-top:0px;
	border:dashed 0px #ddd;
}

.accountContainer .buttons span a{
	color:$blue-link;
	font-size:9px;
}

.accountWrapper.notes{
	margin-top:0px !important;
	width:700px;
	background:#f4f4f4 !important;
}

.accountWrapper .message{
	width:550px;
	margin:0px;
}

.accountForm{
	width:550px;
	padding:0px 15px 15px 15px;
	margin:5px 20px 0px 0px;
}

.accountForm label{
	display:inline-block;
	width:120px;
	text-align:right;
	font-size:14px;
}

.accountForm p{
	margin:10px 0px 20px 0px;
}


.accountContainer .buttons{
	text-align:right;
 	width:330px !important;
	border:solid 0px #ddd;
	margin:0px;
}

.accountContainer .buttons span{
	font-size:9px;
	color:#777;
	font-weight:bold;
}






.paginateButtons{
	margin: 15px;
	text-align:right;
}
.paginateButtons a{
	color:#777; 
	display:inline-block; 
	padding:2px 5px; 
	margin:0px 3px;
	border:dotted 1px #ddd;
	font-size:9px;
}

.paginateButtons .currentStep{
	padding:2px 5px; 
	margin: 5px; 
	background:#f8f8f8; 
	border:solid 1px #ddd;
	font-size:9px;
	font-weight:bold;
}







/** calendar **/
.calendar h1{
	margin:10px 0px 0px 0px;
	vertical-align:middle;
	border:dotted 0px #ddd;
}

.calendar h1 span{
	color:#777;
	font-style:italic;
	font-family:Gill Sans;
}

#calendarFullWrapper{
	padding:10px;
	margin:10px 0px 0px 0px; 
	background:#f4f4f4; 
	border:solid 1px #ebeaea;
	text-align:center;
	height:auto;
}

#calendarFull{
	width:750px !important; 
	height:auto !important; 
	background:#f8f8f8; 
	padding:10px;
	border:solid 0px #fff;
	margin:0px auto;
}


.confirmation{
    width:400px;
	height:270px !important;
	min-height:270px !important;
	margin:20px auto;
	padding:20px;
	text-align:center;				
}
.confirmation h1{
	text-align:center;
}
.confirmation h1 span{
	font-size:30px;
	font-weight:bold;
}
.confirmationStat span{
	font-size:18px;
	font-weight:bold;
	color:#333;
}
.confirmation span.highlight{
	font-size:22px;
}

.confirmation h1 .highlight{
	font-size:30px;
}


